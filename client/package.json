    "@testing-library/user-event": "^12.1.5",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "core-js": "^3.6.5",
    "prettier": "^2.1.2",
    "prettify": "^0.1.7",
    "pretty-quick": "^3.0.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
